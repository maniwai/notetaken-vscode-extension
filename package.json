{
  "name": "notetaken",
  "displayName": "notetaken",
  "private": true,
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "watch": "tsc -watch --project tsconfig.build.json",
    "test": "jest --runInBand --config jest.config.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "activationEvents": [],
  "icon-theme": "vscode",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "x01TreeView",
          "title": "Simple Activity",
          "icon": "./assets/icon.svg"
        }
      ]
    },
    "views": {
      "x01TreeView": [
        {
          "id": "x01Outline",
          "name": "XNote Outile"
        }
      ]
    },
    "commands": [
      {
        "command": "x01Outline.renameNode",
        "title": "Rename"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "x01Outline.renameNode",
          "when": "view == x01Outline"
        }
      ]
    }
  }
}
